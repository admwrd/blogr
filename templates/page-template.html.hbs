<!doctype html>
<html lang="en">
<head>
{{~#if title~}}
<title>{{title}}</title>
{{~else~}}
<title>Vishus Blog</title>
{{/if~}}

<!-- Required meta tags -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<meta name="theme-color" content="#343a40">
<meta name="msapplication-navbutton-color" content="#343a40">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">

{{#if description}}<meta name="description" content="{{description}}">{{/if}}

<link rel="stylesheet" href="{{base_url}}assets/blog-1.0.4.css">
<script src="{{base_url}}assets/before-1.0.3.js"></script>

</head>
<body style="background-image: url(data:image/png;base64,);">
<div id="mainWrapper" class="main-wrapper">

<nav class="v-nav-bar sticky-top navbar navbar-expand-lg navbar-dark bg-dark">
<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
<span class="navbar-toggler-icon"></span>
</button>
<a class="blog-logo navbar-brand" href="{{base_url}}">Vishus Blog</a>
<div class="collapse navbar-collapse" id="navbarSupportedContent">

<ul class="v-nav navbar-nav mr-auto">
    {{~#each menu~}}
      {{~#if this.separator~}}
        <li class="v-nav-item nav-item v-divider {{this.classes}}"></li>
      {{~^~}}
        <li class="v-nav-item nav-item {{this.classes}}"><a class="nav-link" href="{{this.url}}">{{{this.name}}}</a></li>
      {{~/if~}}
    {{~/each~}}
    {{~#if menu_dropdown~}}
    <li class="v-nav-item nav-item dropdown">
        <a class="v-nav-login nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            {{~#if dropdown}}{{dropdown}}{{else}}Menu{{/if}}
        </a>
        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
        {{~#each menu_dropdown~}}
          {{~#if this.separator~}}
              {{~#if this.name~}}
              <h6 class="dropdown-header {{{this.classes}}}">{{{this.name}}}</h6>
              {{~else~}}
              <div class="dropdown-divider {{{this.classes}}}"></div>
              {{~/if}}
          {{~else~}}
              <a class="dropdown-item {{{this.classes}}}" href="{{this.url}}">{{{this.name}}}</a>
          {{~/if}}
        {{~/each~}}
        </div>
    </li>
    {{~/if~}}
</ul>

  <form id="search-form" action="{{base_url}}search" method="get" class="form-inline my-2 my-lg-0">
      <input name="q" class="form-control mr-sm-2" type="search" placeholder="Search" value="{{#if search.q}}{{search.q}}{{/if}}" aria-label="Search">
      <button class="btn btn-outline-primary my-2 my-sm-0" type="submit">
          Search
          <i class="fa fa-search" aria-hidden="true"></i>
      </button>
  </form>
</div>
</nav>

<div id="v-body">
<div class="v-content v-toc-body" style="background: #282C30 url({{base_url}}assets/sphere-trans2.png) no-repeat bottom center fixed;">
<div id="toc"></div>
    {{{body}}}
</div>
<footer id="v-footer">
    <div class="v-copyright"><span class="v-icon-copyright">&copy;</span> 2018 
        <script type="text/javascript" language="javascript">
            show_contact();
        </script><noscript>Andrew Prindle</noscript>
     | <a href="{{base_url}}rss.xml"><i class="v-icon-rss fa fa-rss" aria-hidden="true"></i> RSS</a></div>
    {{~#if gentime~}}
    <div class="v-generation-time">
        <i class="v-icon-pagegen fa fa-tachometer" aria-hidden="true"></i>
        Generated in {{gentime}}</div>
    {{~/if~}}
</footer>
</div>
</div>
<script src="{{base_url}}assets/after-1.0.4.js"></script>
<script>
  enable_toc();
{{~#if js~}}
{{{js}}}
{{~/if~}}
</script>
</body>
</html>
